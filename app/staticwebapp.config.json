{
    "routes": [
        {
            "route": "/profile",
            "allowedRoles": [
                "Red"
            ]
        },
        {
            "route": "/login",
            "redirect": "/.auth/login/aad?post_login_redirect_uri=/index.html%23fragment"
        },
        {
            "route": "/about",
            "redirect": "/#about"
        },
        {
            "route": "/logout",
            "rewrite": "/.auth/logout?post_logout_redirect_uri=/profile.html%23fragment"
        },
        {
            "route": "/index",
            "headers": {
                "X-Robots-Tag": "mishapos-custom-robots-header"
            }
        }
    ]
}
